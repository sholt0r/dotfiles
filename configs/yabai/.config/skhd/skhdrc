# Windows
# Switch
lcmd - h : yabai -m window --focus west
lcmd - j : yabai -m window --focus south
lcmd - k : yabai -m window --focus north
lcmd - l : yabai -m window --focus east

# Move
lshift + lcmd - h : yabai -m window --swap west
lshift + lcmd - j : yabai -m window --swap south
lshift + lcmd - k : yabai -m window --swap north
lshift + lcmd - l : yabai -m window --swap east

# Spaces
# Switch
cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4
cmd - 5 : yabai -m space --focus 5
cmd - 6 : yabai -m space --focus 6
cmd - 7 : yabai -m space --focus 7
cmd - 8 : yabai -m space --focus 8
cmd - 9 : yabai -m space --focus 9
cmd - 0 : yabai -m space --focus 10

# Move
lshift + lcmd - 1 : yabai -m window --space 1 --focus
lshift + lcmd - 2 : yabai -m window --space 2 --focus
lshift + lcmd - 3 : yabai -m window --space 3 --focus
lshift + lcmd - 4 : yabai -m window --space 4 --focus
lshift + lcmd - 5 : yabai -m window --space 5 --focus
lshift + lcmd - 6 : yabai -m window --space 6 --focus
lshift + lcmd - 7 : yabai -m window --space 7 --focus
lshift + lcmd - 8 : yabai -m window --space 8 --focus
lshift + lcmd - 9 : yabai -m window --space 9 --focus
lshift + lcmd - 0 : yabai -m window --space 10 --focus

# View
lshift + lcmd - v : yabai -m space --rotate 270
lshift + lcmd - t : yabai -m window --toggle float --grid 4:4:1:1:2:2
lshift + lcmd - f : yabai -m window --toggle zoom-fullscreen
